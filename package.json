{
  "name": "kompis-workspace",
  "version": "0.0.0",
  "license": "MIT",
  "description": "Kompis.js is an open source tool for developing React UI components in isolation",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/kompisjs/kompis.git"
  },
  "workspaces": [
    "exmaples/*"
  ],
  "scripts": {
    "next": "yarn workspace @kompis/next-example",
    "dev": "concurrently -n web 'yarn next dev'"
  },
  "keywords": [
    "components",
    "documentation",
    "frontend",
    "kompis",
    "kompisjs",
    "react",
    "reactjs"
  ],
  "devDependencies": {
    "concurrently": "^6.5.1",
    "eslint": "^8.4.1",
    "prettier": "^2.5.1"
  }
}
